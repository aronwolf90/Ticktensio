# frozen_string_literal: true

module <%= class_name.pluralize.split("::")[0..-2].join("::") %>
  module <%= class_name.pluralize.split("::").last.pluralize %>::Cell
    class Form < BaseFormCell
      def actions(form)
        super(
          form,
          cancell_path: %i[<%= class_path.join(' ') %> <%=  class_name.split("::").last.underscore.pluralize %>],
          destroy_path: [<%= class_path.map { |e| ":#{e}" }.join(', ') %>, model]
        )
      end
    end
  end
end

{
  "name": "template_project",
  "private": true,
  "dependencies": {
    "@rails/webpacker": "^3.0.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bootstrap": "4.0.0-beta.2",
    "bootstrap-vue": "^1.3.0",
    "bootstrap.native": "^2.0.21",
    "chai": "^4.1.2",
    "coffeescript": "1.12.7",
    "cross-env": "^5.1.3",
    "es2016": "^0.0.0",
    "expect": "^22.0.6",
    "jest-serializer-vue": "^0.3.0",
    "jsdom": "^11.5.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^4.1.0",
    "mocha-webpack": "^1.0.1",
    "nyc": "^11.4.1",
    "pug": "^2.0.0-rc.4",
    "sinon": "^4.1.6",
    "sinon-chai": "^2.14.0",
    "tui-editor": "^1.0.0",
    "vue": "^2.5.9",
    "vue-awesome": "^2.3.4",
    "vue-jest": "^1.4.0",
    "vue-loader": "^13.5.0",
    "vue-resource": "^1.3.4",
    "vue-router": "^3.0.1",
    "vue-search-select": "^2.4.0",
    "vue-store": "^0.3.1-moved",
    "vue-template-compiler": "^2.5.9",
    "vue-test-utils": "^1.0.0-beta.11",
    "vue-turbolinks": "^2.0.0",
    "vuedraggable": "^2.15.0",
    "vuex": "^3.0.1",
    "webpack-node-externals": "^1.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.0.6",
    "babel-preset-es2015": "^6.24.1",
    "jest": "^22.0.6",
    "jest-vue-preprocessor": "^1.3.1",
    "regenerator-runtime": "^0.11.1",
    "webpack-dev-server": "^2.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "app/javascript"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "mapCoverage": true
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc mocha-webpack --webpack-config config/webpack/test.js --require spec/mocha.js spec/javascript/**/*.spec.js"
  },
  "nyc": {
    "include": [
      "src/**/*.(js|vue)"
    ],
    "instrument": false,
    "sourceMap": false
  }
}
